<settings>
    <setting propertyKey="@Properties.target_latitude" title="Target Latitude">
        <settingConfig type="numeric" errorMessage="@Strings.MyNumberError" min="-90" max="90" />
    </setting>
    <setting propertyKey="@Properties.target_longitude" title="Target Longitude">
        <settingConfig type="numeric" errorMessage="@Strings.MyNumberError" min="-180" max="180" />
    </setting>
    <setting propertyKey="@Properties.radius" title="Radius">
        <settingConfig type="numeric" errorMessage="@Strings.MyNumberError" min="3" max="100" />
    </setting>
</settings>